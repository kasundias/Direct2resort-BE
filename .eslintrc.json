{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "node-style-guide"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "space-after-keywords": "off",
        "keyword-spacing": [
            2,
            {
                "before": true,
                "after": true
            }
        ]
    }
    
}